   "source": [
    "%matplotlib inline\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "n0 = 5.0 / 4\n",
    "t12 = 5.0\n",
    "days_to_calc = 50\n",
    "\n",
    "def amount(t):\n",
    "    return n0 * pow(0.5, t / t12)\n",
    "\n",
    "x = range(days_to_calc)\n",
    "y = [0] * days_to_calc\n",
    "\n",
    "for \n",
    "#y = map(amount, x)\n",
    "\n",
    "plt.scatter(x, y)\n",
    "plt.show()"
   ]
